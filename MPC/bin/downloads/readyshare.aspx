    /* GENERAL STYLES */
* {
	padding:0;
	margin:0;
	outline:none;
	border:none;
	font-family:arial;
}
html, body, div, p, form, table, th, td, p, h1, ul, li, span {
	padding:0;
	margin:0;
	outline:none;
	border:none;
	font-family:Arial;
}
body {
	background:#050505 url(/assets/landing/readyshare/body_bkg_rpt.jpg) repeat-x top left;
}
img {
	border:0px;
}
a {
	outline:none;
}
a:focus {
	outline:0;
}
.bold {
	font-weight:bold;
}
#wapper {
	width:1100px;
	margin:auto;
	overflow:hidden;
	background:url(/assets/landing/readyshare/body_bkg.jpg) no-repeat top left;
}
#container {
	float:left;
	width:955px;
	padding-bottom:15px;
	margin-left:78px
}
.equal2 {
	height: 295px;
}
.equal3	{ height: 215px; }
#row-1, #row-2, #row-3	{ overflow: hidden; }
/* HEADER BAR */
#header_bar {
	width:955px;
	height:81px;
	float:left;
	position:relative;
	background:url(/assets/landing/readyshare/header_bar.png) no-repeat;
}
.logo {
	margin:15px 0 0 40px;
}
/*CONTENT AREA*/
#content {
	width:955px;
	margin:230px 0px 0px;
	float:left;
	background:#FFFFFF;
}
#content-inner {
	margin:15px 35px;
}
.content-section {
	margin-bottom:25px;
	background:#EBEBEB;
	border: 1px solid #d1d5d7;
	border-top:none;
	width: 425px;
}
.content-section .content-h1 {
	height:34px;
	font-size:16px;
	color:#ffffff;
	background:url(/assets/landing/readyshare/heading_bar_bkg.jpg) repeat-x top left;
	line-height:35px;
	padding-left:18px;
}
.content-section p {
	color:#302f30;
	font-size:12px;
	line-height: 16px;
	margin-left:18px;
	width:425px;
	_height:400px;
}
.content-section p.first {
	padding-top:5px;
}
.content-section p.last {
	margin-bottom: 10px;
}
.content-section ul {
	margin:0px 0px 15px 26px;
}
.content-section ul li {
	list-style:none;
	font-size:12px;
	font-weight:bold;
	color:#303030;
	padding:3px;
}
.content-section ul li span.raquo {
	margin-right:5px;
	font-size:12px;
}
.content-section ul li a {
	text-decoration:none;
	color:#303030;
}
.content-section ul li a:hover {
	text-decoration:underline;
}
.content-section .content-supported {
	color:#676767;
	font-size:11px;
}
.content-section .content-supported a {
	color:#676767;
	text-decoration:none;
}
.content-section .content-supported a:hover {
	text-decoration:underline;
}
.content-section p.readyshare-storage {
	float:right;
	width:176px;
	margin:15px 35px 0px 0px;
	text-align: center;
}
.content-section p.dlna {
	float:right;
	width:184px;
	margin:59px 56px 0px 0px;
	text-align: center;
}
.content-section p.readyshare-print {
	float:right;
	width:192px;
	margin:23px 33px 0px 0px;
	text-align: center;
}
.content-section p.readyshare-cloud {
	float:right;
	width:164px;
	margin:15px 45px 0px 0px;
	text-align: center;
}
.content-section p.readyshare-vault {
	float:right;
	width:65px;
	margin:15px 45px 0px 0px;
	text-align: center;
}
.content-section p.readyshare_apps {
	font-size:10px;
	color:#5e5d5d;
	position: relative;
	left:230px;
	top:-50px;
	width:159px;
}
.content-section #timemachine ul {
	margin:8px 0px 15px 26px;
}
.content-section #timemachine ul li {
	list-style:none;
	font-size:12px;
	font-weight:normal;
	color:#676767;
	padding:3px;
	line-height:18px;
}
/*FOOTER SECTION*/
#footer {
	width:955px;
	margin:20px 13px;
	float:left;
}
.text {
	width:690px;
	margin-left:10px;
	color:#ffffff;
	font:normal 11px arial;
	float:left;
}
.social {
	margin-right:55px;
	float:right;
}
/* LIGHTBOX STYLES */

#colorbox, #cboxOverlay, #cboxWrapper {
	position: absolute;
	top: 0;
	left: 0;
	z-index: 9999;
	overflow: hidden;
}
#cboxOverlay {
	position: fixed;
	width: 100%;
	height: 100%;
}
#cboxMiddleLeft, #cboxBottomLeft {
	clear: left;
}
#cboxContent {
	position: relative;
	overflow: hidden;
}
#cboxLoadedContent {
	overflow: auto;
}
#cboxLoadedContent iframe {
	display: block;
	width: 100%;
	height: 100%;
	border: 0;
}
#cboxTitle {
	margin: 0;
}
#cboxLoadingOverlay, #cboxLoadingGraphic {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
}
#cboxPrevious, #cboxNext, #cboxClose, #cboxSlideshow {
	cursor: pointer;
}
#cboxOverlay {
	background: #000;
}
#colorBox {
}
#cboxTopLeft {
	width: 25px;
	height: 25px;
	background: url(/upload/colorbox/images/border1.png) 0 0 no-repeat;
}
#cboxTopCenter {
	height: 25px;
	background: url(/upload/colorbox/images/border1.png) 0 -50px repeat-x;
}
#cboxTopRight {
	width: 25px;
	height: 25px;
	background: url(/upload/colorbox/images/border1.png) -25px 0 no-repeat;
}
#cboxBottomLeft {
	width: 25px;
	height: 25px;
	background: url(/upload/colorbox/images/border1.png) 0 -25px no-repeat;
}
#cboxBottomCenter {
	height: 25px;
	background: url(/upload/colorbox/images/border1.png) 0 -75px repeat-x;
}
#cboxBottomRight {
	width: 25px;
	height: 25px;
	background: url(/upload/colorbox/images/border1.png) -25px -25px no-repeat;
}
#cboxMiddleLeft {
	width: 25px;
	background: url(/upload/colorbox/images/border2.png) 0 0 repeat-y;
}
#cboxMiddleRight {
	width: 25px;
	background: url(/upload/colorbox/images/border2.png) -25px 0 repeat-y;
}
#cboxContent {
	background: #fff;
}
#cboxLoadedContent {
	margin-bottom: 20px;
}
#cboxTitle {
	position: absolute;
	bottom: 0px;
	left: 0;
	text-align: center;
	width: 100%;
	color: #999;
}
#cboxCurrent {
	position: absolute;
	bottom: 0px;
	left: 100px;
	color: #999;
}
#cboxSlideshow {
	position: absolute;
	bottom: 0px;
	right: 42px;
	color: #444;
}
#cboxPrevious {
	position: absolute;
	bottom: 0px;
	left: 0;
	color: #444;
}
#cboxNext {
	position: absolute;
	bottom: 0px;
	left: 63px;
	color: #444;
}
#cboxLoadingOverlay {
	background: url(/upload/colorbox/images/loading.gif) 5px 5px no-repeat #fff;
}
#cboxClose {
	position: absolute;
	bottom: 0;
	right: 0;
	display: block;
	color: #444;
}
